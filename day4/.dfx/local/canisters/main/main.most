// Version: 1.0.0
type Proposal =
  {
    creator : Principal;
    downVotes : Nat;
    id : Nat;
    motion : Text;
    status : Status;
    upVotes : Nat
  };
type Status = {#Accepted; #Pending; #Rejected};
actor {
  stable var proposalCurrentID : Nat;
  stable var stableProposals : [(Nat, Proposal)]
};
